    "@types/markdown-it-anchor": "^7.0.0",
    "autoprefixer": "^10.2.4",
    "browserslist": "^4.16.3",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-postcss": "^9.0.0",
